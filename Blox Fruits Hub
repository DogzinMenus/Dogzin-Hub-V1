-- Kavo UI local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))() local Window = Library.CreateLib("DogzinHub V1 - Blox Fruits", "Ocean")

-- Tabs local TabMain = Window:NewTab("Main") local TabFarm = Window:NewTab("Auto Farm") local TabBoss = Window:NewTab("Bosses") local TabMastery = Window:NewTab("Mastery") local TabTP = Window:NewTab("Teleport") local TabMisc = Window:NewTab("Misc") local TabVisual = Window:NewTab("Visual")

-- Fast Attack TabMain:NewToggle("Ativar Fast Attack", "Ativa ataques rÃ¡pidos", function(v) _G.FastAttack = v while _G.FastAttack do pcall(function() local plr = game.Players.LocalPlayer local char = plr.Character local tool = char:FindFirstChildOfClass("Tool")

if tool then
			local args = {
				[1] = {
					["HRP"] = char:FindFirstChild("HumanoidRootPart"),
					["Tool"] = tool
				}
			}
			game:GetService("ReplicatedStorage").Remotes.Combat:FireServer(unpack(args))
		end
	end)
	task.wait(0.15)
end

end)

-- Boss Dropdown TabBoss:NewDropdown("Selecionar Boss", "Escolha um Boss para farmar", { "Bandit Boss", "Gorilla King", "Bobby", "Yeti", "Magma Admiral", "Flamingo" }, function(selected) _G.SelectedBoss = selected end)

-- Auto Kill Boss TabBoss:NewToggle("Auto Kill Boss", "Ataca automaticamente o boss selecionado", function(v) _G.AutoKillBoss = v

while _G.AutoKillBoss do
	pcall(function()
		local player = game.Players.LocalPlayer
		local char = player.Character or player.CharacterAdded:Wait()
		local tool = char:FindFirstChildOfClass("Tool")
		local hrp = char:WaitForChild("HumanoidRootPart")
		local boss = nil

		for i, v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
			if v:FindFirstChild("HumanoidRootPart") and v.Name:lower():match(_G.SelectedBoss:lower()) then
				boss = v
				break
			end
		end

		if boss and boss:FindFirstChild("Humanoid") and boss.Humanoid.Health > 0 then
			hrp.CFrame = boss.HumanoidRootPart.CFrame * CFrame.new(0, 10, 0)

			if tool then
				game:GetService("ReplicatedStorage").Remotes.Combat:FireServer({
					["Tool"] = tool,
					["HRP"] = hrp
				})
			end
		end
	end)
	task.wait(0.15)
end

end)

-- Auto Farm Mastery TabMastery:NewToggle("Auto Farm Mastery", "Farmar maestria automaticamente", function(v) _G.FarmMastery = v

while _G.FarmMastery do
	pcall(function()
		local player = game.Players.LocalPlayer
		local char = player.Character or player.CharacterAdded:Wait()
		local tool = char:FindFirstChildOfClass("Tool")
		local hrp = char:WaitForChild("HumanoidRootPart")
		local target = nil

		for i, v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
			if v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 and v:FindFirstChild("HumanoidRootPart") then
				target = v
				break
			end
		end

		if target then
			hrp.CFrame = target.HumanoidRootPart.CFrame * CFrame.new(0, 10, 0)

			if tool then
				game:GetService("ReplicatedStorage").Remotes.Combat:FireServer({
					["Tool"] = tool,
					["HRP"] = hrp
				})
			end
		end
	end)
	task.wait(0.15)
end

end)

