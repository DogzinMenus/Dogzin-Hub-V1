return function()
    -- Carrega a UI do RedzHub (voc√™ mant√©m tudo igual aqui)
    local RedzUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/RedzHub/RedzHubV2/main/Lib/UI.lua"))()

    -- Cria a janela principal com o nome modificado
    RedzUI:CreateWindow({
        Name = "DogzinHub V1", -- üîÅ Aqui muda o nome
        LoadingTitle = "DogzinHub V1",
        LoadingSubtitle = "By DogzinGhost_YT",
        ConfigurationSaving = {
            Enabled = true,
            FolderName = "DogzinHubV1",
            FileName = "DogzinHubConfig"
        },
        Discord = {
            Enabled = false,
            Invite = "seuInviteAqui",
            RememberJoins = false
        },
        KeySystem = false
    })

    -- Continua com as abas, toggles, e fun√ß√µes como no RedzHub
    local Tab = RedzUI:MakeTab({
        Name = "Main",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })

    Tab:AddToggle({
        Name = "Fast Attack",
        Default = false,
        Callback = function(Value)
            _G.FastAttack = Value
            while _G.FastAttack and task.wait() do
                pcall(function()
                    local tool = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool")
                    if tool then
                        tool:Activate()
                    end
                end)
            end
        end
    })

    -- Adicione mais funcionalidades como quiser
end
